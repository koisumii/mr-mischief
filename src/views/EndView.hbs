{{> Header }}

<div class="end-container">
    <div class="end-background"></div>
    <h2>Well, look who made it to the end! Not too shabby, huh?</h2>
    <div class="score-container">
        <p>Your score: {{score}}</p>
        <p>Highest score: {{highScore}}</p>
        <div class="quizmaster-end">
            <img src="/mischief-expressions/coin-flipping.gif" alt="Quizmaster end image">
        </div>
    </div>
    <div class="button-container">
        <a href="/start" class="button">Play Again</a>
        <a href="/" class="button">Return</a>
    </div>
</div>

{{> Footer }}
